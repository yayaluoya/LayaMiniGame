        ".rpt2_cache/**": true, //laya缓存
        ".laya/chrome/**": true, //谷歌插件代码
        ".laya/**": true, //laya自带
        "node_modules/**": true, //npm 包文件夹
        ".node/**": true, //node工具代码
        "public/**": true, //
